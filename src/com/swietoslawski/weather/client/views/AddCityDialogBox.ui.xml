<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.panel {
			background-color: ivory;
		}
	</ui:style>

	<g:FlowPanel styleName="{style.panel}">
		<!-- Search box and input field  -->
		<g:TextBox ui:field="city">search...</g:TextBox>
		<g:Button ui:field="search">?</g:Button>
		<g:HTML ui:field="message"></g:HTML>
		<g:Button ui:field="add">Add</g:Button>
		<g:Button ui:field="cancel">Cancel</g:Button>
	</g:FlowPanel>	
</ui:UiBinder>
