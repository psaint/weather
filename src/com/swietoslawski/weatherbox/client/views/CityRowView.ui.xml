<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.panel {
			background-color: ivory;
		}
	</ui:style>

	<g:FlowPanel styleName="row">
		<g:HorizontalPanel width="100%" height="48px">
			<g:Cell verticalAlignment="ALIGN_MIDDLE" width="80%">
				<g:Label text="city" ui:field="city"/>
			</g:Cell>
			<g:Cell verticalAlignment="ALIGN_MIDDLE" width="20%">
				<g:Button ui:field="delete">delete</g:Button>
			</g:Cell>
		</g:HorizontalPanel>
	</g:FlowPanel>
</ui:UiBinder>
