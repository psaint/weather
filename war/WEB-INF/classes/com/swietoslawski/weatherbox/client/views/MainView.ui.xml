<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.panel {
			background-color: ivory;
		}
	</ui:style>

	
		<g:DockLayoutPanel unit="PT" width="100%" height="100%" styleName="main-wrapper">
			<g:north size="10.0">
				<g:FlowPanel styleName="header">
					<g:Label text=""/>
				</g:FlowPanel>
			</g:north>
			<g:south size="73.6">
				<g:FlowPanel height="80pt">
					<g:FlowPanel styleName="pager">
						<g:HTML ui:field="pager"></g:HTML>
					</g:FlowPanel>
					<g:FlowPanel styleName="toolbar">
						<g:PushButton text="H" ui:field="home"/>
						<g:PushButton text="+" ui:field="add"/>
						<g:PushButton text="..." ui:field="list"/>
						<g:PushButton text="@" ui:field="update"/>
					</g:FlowPanel>
				</g:FlowPanel>
			</g:south>
			<g:west size="20">
				<g:PushButton text="&nbsp;" ui:field="prev" styleName="prev"/>
			</g:west>
			<g:east size="20">
				<g:PushButton text="&nbsp;" ui:field="next" styleName="next"/>
			</g:east>
			<g:center styleName="content">
				<g:FlowPanel ui:field="content" styleName="content">
					<g:Label text="Content" horizontalAlignment="ALIGN_CENTER"/>
				</g:FlowPanel>
			</g:center>
		</g:DockLayoutPanel>
</ui:UiBinder>
