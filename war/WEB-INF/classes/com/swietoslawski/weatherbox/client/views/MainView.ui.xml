<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.panel {
			background-color: ivory;
		}
	</ui:style>

	
		<g:DockLayoutPanel unit="PCT" width="100%" height="100%">
			<g:north size="8.5">
				<g:FlowPanel>
					<g:Label text="Header" horizontalAlignment="ALIGN_CENTER"/>
				</g:FlowPanel>
			</g:north>
			<g:south size="19.2">
				<g:HorizontalPanel horizontalAlignment="ALIGN_CENTER" width="100%" height="40px" verticalAlignment="ALIGN_MIDDLE">
					<g:PushButton text="Home" width="40px" ui:field="home"/>
					<g:PushButton text="Add" width="40px" ui:field="add"/>
					<g:PushButton text="List" width="40px" ui:field="list"/>
					<g:PushButton text="Help" width="40px" ui:field="help"/>
				</g:HorizontalPanel>
			</g:south>
			<g:west size="7.1">
				<g:PushButton text="&lt;" ui:field="prev"/>
			</g:west>
			<g:east size="7.1">
				<g:PushButton text="&gt;" ui:field="next"/>
			</g:east>
			<g:center>
				<g:FlowPanel ui:field="content">
					<g:Label text="Content" horizontalAlignment="ALIGN_CENTER"/>
				</g:FlowPanel>
			</g:center>
		</g:DockLayoutPanel>
	
</ui:UiBinder>
