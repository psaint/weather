<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.button {
			font-weight: bold;
			height: 40pt;
			width: 40pt;
		}
	</ui:style>
	
	<g:HTMLPanel>
		<g:Button styleName="{style.button}" ui:field="add" />
		<g:Button styleName="{style.button}" ui:field="list" />
		<g:Button styleName="{style.button}" ui:field="config" />
		<g:Button styleName="{style.button}" ui:field="help" />
		<g:DockLayoutPanel unit="EM" width="150px" height="100px"/>
	</g:HTMLPanel>
	
</ui:UiBinder> 